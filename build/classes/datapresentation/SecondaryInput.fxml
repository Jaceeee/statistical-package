<AnchorPane id="AnchorPane" prefHeight="392.0" prefWidth="448.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="datapresentation.MainController">
   <children>
      <Label alignment="CENTER" layoutX="126.0" layoutY="34.0" prefHeight="37.0" prefWidth="105.0" text="Input Data">
