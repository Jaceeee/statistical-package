         <effect>
            <ColorAdjust hue="-0.79" saturation="0.39" />
         </effect></Button>
      <Button fx:id="proceed3" layoutX="343.0" layoutY="243.0" mnemonicParsing="false" onAction="#handleInputAction3" prefHeight="37.0" prefWidth="67.0" text="Proceed">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <ColorAdjust hue="-0.79" saturation="0.39" />
         </effect></Button>
      <Button fx:id="back2" layoutX="343.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleBackAction2" prefHeight="37.0" prefWidth="67.0" text="Back">
