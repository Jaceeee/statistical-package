            <Font name="Lucida Sans Regular" size="14.0" />
         </font></Label>
      <TextField fx:id="dataField" layoutX="104.0" layoutY="83.0" onKeyPressed="#enterListener" />
      <Button fx:id="enter" layoutX="295.0" layoutY="83.0" mnemonicParsing="false" onAction="#handleEnterInputAction" prefHeight="25.0" prefWidth="67.0" text="Enter">
         <cursor>
            <Cursor fx:constant="HAND" />
