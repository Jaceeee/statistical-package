      <Label alignment="CENTER" layoutX="189.0" layoutY="161.0" prefHeight="17.0" prefWidth="115.0" text="Input" />
      <ListView fx:id="numbers" layoutX="51.0" layoutY="192.0" minWidth="-Infinity" prefHeight="160.0" prefWidth="127.0" />
      <ListView fx:id="inputDisplay" layoutX="178.0" layoutY="192.0" prefHeight="160.0" prefWidth="118.0" />
      <Label fx:id="title" alignment="CENTER" contentDisplay="CENTER" layoutX="104.0" layoutY="132.0" prefHeight="17.0" prefWidth="149.0">
         <font>
            <Font name="Lucida Bright Regular" size="14.0" />
         </font>
      </Label>
